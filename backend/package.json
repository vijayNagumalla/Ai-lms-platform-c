{
  "name": "lms-backend",
  "version": "1.0.0",
  "description": "LMS Platform Backend API",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "migrate": "node scripts/runMigrations.js",
    "migrate:status": "node -e \"import('./utils/migrationRunner.js').then(m => m.default.getMigrationStatus().then(s => console.table(s)))\"",
    "migrate:mysql-to-supabase": "node scripts/migrate-mysql-to-supabase.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.5",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "formidable": "^3.5.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "judge0-client": "^0.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "prop-types": "^15.8.1",
    "puppeteer": "^24.22.3",
    "react-hot-toast": "^2.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11"
  },
  "keywords": [
    "lms",
    "education",
    "api",
    "mysql"
  ],
  "author": "LMS Platform",
  "license": "MIT"
}
